function getRndInteger(min, max) {
  return Math.floor(Math.random() * (max - min) ) + min;
}
	
function Generate() {


while (KingField == RookField1 || KingField == RookField2 || RookField1 == RookField2 || 
		randomFieldCharKing == randomFieldCharRook1 || randomFieldCharKing == randomFieldCharRook2
		|| randomFieldCharRook1 == randomFieldCharRook2 || randomFieldNumberKing == randomFieldNumberRook1
		|| randomFieldNumberKing == randomFieldNumberRook2 || randomFieldNumberRook1 == randomFieldNumberRook2 ||
		KingFieldW == RookField1 || KingFieldW == RookField2 || KingFieldW == KingField || 
		randomFieldCharKingW == randomFieldCharRook1 || randomFieldCharKingW == randomFieldCharRook2
		|| randomFieldNumberKingW == randomFieldNumberRook1
		|| randomFieldNumberKingW == randomFieldNumberRook2)
{

